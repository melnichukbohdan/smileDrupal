<?php

use Drupal\Core\Form\FormStateInterface;

/**
 * Implements hook_form_alter().
 */
function pets_owners_form_alter(&$form, FormStateInterface $form_state, $form_id) {
  if($form_id == 'search_block_form') {
    $form['keys']['#attributes']['placeholder'] = t('Enter some text to search');
  }
}
